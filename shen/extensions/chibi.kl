(defun open-append (V1266) (let FullPath (scm.path-resolve V1266 (value *home-directory*)) (let Mode (scm. "(+ open/create open/write open/append)") (let Fd (scm.open FullPath Mode) (scm.open-output-file-descriptor Fd)))))



