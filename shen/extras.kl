(defun shen.quiet-load (V1178) (let Contents (read-file V1178) (map (lambda X (shen.eval-without-macros X)) Contents)))

(defun sterror () (value *sterror*))

(defun exit (V1180) (scm.exit V1180))

(defun command-line () (scm.command-line))

(defun open-append (V1182) (let FullPath (scm.path-resolve V1182 (value *home-directory*)) (let Mode (scm. "(scm.+ scm.open/create scm.open/write scm.open/append)") (let Fd (scm.open FullPath Mode) (scm.open-output-file-descriptor Fd)))))

(declare open-append (cons string (cons --> (cons (cons stream (cons out ())) ()))))



